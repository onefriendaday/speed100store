  <script src="{{ 'assets/js/vendor/up.js' | asset_url }}"></script>
  <script>
    UpUp.debug(true);
    UpUp.start({
      'content': '<html><body><h1>You are offline now</h1><p>Content for offline</p></body></html>',
      'service-worker-url': '/service-worker.js?v'
    });
  </script>
</body>
</html>
